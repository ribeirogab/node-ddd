{
  "extends": [
    "@ribeirogab/eslint-config/node",
    "plugin:vitest-globals/recommended"
  ],
  "env": {
    "vitest-globals/env": true
  },
  "overrides": [
    {
      "files": [
        "*"
      ],
      "rules": {
        "@typescript-eslint/adjacent-overload-signatures": "off"
      }
    },
    {
      "files": [
        "vite.config.ts"
      ],
      "rules": {
        "import/no-default-export": "off"
      }
    }
  ]
}